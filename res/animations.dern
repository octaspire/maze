(animation-entity-define [player-idle]          0)
(animation-entity-define [exit-idle]            1)
(animation-entity-define [stopper-idle]         2)
(animation-entity-define [pullable-ensw-idle]   3)
(animation-entity-define [wall-idle]           18)


(define create-stop-counter-i [.] '(i [i]) (fn (i)
    (define id [id] (string-format [stop-counter-{}] i))
    (animation-entity-define id (+ 131 i))))

(for i from 1 to 99 (create-stop-counter-i i))


